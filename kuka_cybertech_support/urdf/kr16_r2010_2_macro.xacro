      <visual>
        <geometry>
          <mesh filename="package://kuka_cybertech_support/meshes/kr16_r2010_2/visual/base_link.stl"/>
        </geometry>
        <origin rpy="-0.0 0.0 -0.0" xyz="0.0 0.0 0.0"/>
      </visual>
    </link>
    <link name="${prefix}link_1">
      <collision>
        <geometry>
          <box size="0.4838663190603256 0.5623050630092621 0.4337000250816345"/>
        </geometry>
        <origin rpy="0.0 0.0 0.0" xyz="0.0559331551194191 -0.0335475318133831 -0.47484999895095825"/>
      </collision>
      <inertial>
        <inertia ixx="9.85967888" ixy="0.0" ixz="0.0" iyy="0.91499458" iyz="0.0" izz="10.23026596"/>
        <mass value="8.4207E1"/>
        <origin rpy="-0.0 0.0 -0.0" xyz="0.0 0.0 0.0"/>
      </inertial>
